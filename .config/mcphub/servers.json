{
  "mcpServers": {
    "docker": {
      "command": "uvx",
      "args": [
        "mcp-server-docker"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx"
    },
    "github": {
      "command": "npx",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx"
    },
    "serena": {
      "command": "/home/ir/.local/bin/uv",
      "env": {
        "HOME": null
      },
      "args": [
        "run",
        "--directory",
        "/home/ir/Software/serena",
        "serena-mcp-server",
        "--project-file",
        "/home/ir/Software/serena/myproject.yml"
      ]
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "perplexity": {
      "command": "node",
      "env": {
        "HOME": null,
        "PERPLEXITY_API_KEY": null
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      },
      "command": "npx"
    }
  },
  "nativeMCPServers": [
    
  ]
}
