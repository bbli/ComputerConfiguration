{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "github": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx"
    },
    "docker": {
      "command": "uvx",
      "args": [
        "mcp-server-docker"
      ]
    },
    "serena": {
      "env": {
        "HOME": null
      },
      "args": [
        "run",
        "--directory",
        "/home/ir/Software/serena",
        "serena-mcp-server"
      ],
      "command": "/home/ir/.local/bin/uv"
    },
    "perplexity": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "command": "npx"
    },
    "puppeteer": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  }
}