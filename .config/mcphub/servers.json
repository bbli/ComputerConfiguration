{
  "mcpServers": {
    "openai-swarm": {
      "url": "https://gitmcp.io/openai/swarm"
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx"
    },
    "puppeteer": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx"
    },
    "docker": {
      "args": [
        "mcp-server-docker"
      ],
      "command": "uvx"
    },
    "rust-lsp": {
      "args": [
        "--workspace",
        "/Users/beli/LogParser",
        "--lsp",
        "rust-analyzer"
      ],
      "command": "mcp-language-server"
    },
    "serena": {
      "args": [
        "run",
        "--directory",
        "/Users/beli/Software/serena",
        "serena-mcp-server",
        "--project-file",
        "/Users/beli/Software/serena/project.template.yml"
      ],
      "command": "/Users/beli/.local/bin/uv"
    },
    "perplexity": {
      "args": [
        "/Users/beli/Documents/Claude/MCP/perplexity-mcp/build/index.js"
      ],
      "env": {
        "PERPLEXITY_API_KEY": null
      },
      "command": "node"
    },
    "github": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      },
      "command": "npx"
    }
  },
  "nativeMCPServers": [
    
  ]
}